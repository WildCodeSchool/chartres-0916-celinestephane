   function getStyle(el,styleProp){
      var x = document.getElementById(el);
      if (x.currentStyle)
         var y = x.currentStyle[styleProp];
      else if (window.getComputedStyle)
         var y = document.defaultView.getComputedStyle(x,null).getPropertyValue(styleProp);
      return y;
   }

   function colorToHex(color) {
      if (color.substr(0, 1) === '#') {
         return color;
      }
      if (color.match(/(.*?)rgb\((\d+), (\d+), (\d+)\)/)){
         var r = parseInt(RegExp.$2);
         var g = parseInt(RegExp.$3);
         var b = parseInt(RegExp.$4);
         return "#" + componentToHex(r) + componentToHex(g) + componentToHex(b);
      }
   }

   function componentToHex(c) {
      var hex = c.toString(16);
      return hex.length == 1 ? "0" + hex : hex;
   }

   if ('font-size:12px; font-family:comic sans ms,comic-sans-w01-regular,comic-sans-w02-regular,comic-sans-w10-regular,cursive; color:rgba(210,176,7,1); background-color:rgba(45,37,37,0); letter-spacing:10px; display:inline-block; min-width:110px; padding:0px 13px; border:solid 0px rgba(221,119,11,0.123); border-radius:0px; text-decoration:none;' != ''){
